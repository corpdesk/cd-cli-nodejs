{
  "name": "@corpdesk/cd-cli",
  "version": "0.1.1",
  "type": "module",
  "description": "Corpdesk CLI to scaffold, generate, test, and publish modules for cd-shell",
  "author": "George Oremo <george.oremo@gmail.com>",
  "license": "MIT",
  "bin": {
    "cd-cli": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "README.md",
    "templates",
    "bin"
  ],
  "scripts": {
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint . --fix",
    "build": "tsc --project tsconfig.json",
    "watch": "tsc --watch --project tsconfig.json",
    "changeset": "changeset",
    "versions": "changeset version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "cli",
    "corpdesk",
    "module-generator",
    "cd-shell",
    "pwa",
    "typescript",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/corpdesk/cd-cli-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/corpdesk/cd-cli-nodejs.git/issues"
  },
  "homepage": "https://github.com/corpdesk/cd-cli-nodejs.git#readme",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/testing": "^10.4.15",
    "@socket.io/redis-adapter": "^8.3.0",
    "@swc/core": "^1.10.7",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "class-validator": "^0.14.1",
    "cli-table3": "^0.6.5",
    "cloudmailin": "^0.1.0",
    "commander": "^12.1.0",
    "dayjs": "^1.11.13",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.4.7",
    "html-to-text": "^9.0.5",
    "inquirer": "^9.1.0",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "logger": "^0.0.1",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "mysql2": "^3.14.1",
    "node-cleanup": "^2.1.2",
    "node-ssh": "^13.2.0",
    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "nodemailer": "^6.10.0",
    "path": "^0.12.7",
    "puppeteer": "^24.4.0",
    "pusher": "^5.2.0",
    "redis": "^4.7.0",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3",
    "update-notifier": "^7.3.1",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.4"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coverageDirectory": "./coverage",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/utils/test-utils.ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!chalk)"
    ]
  }
}
